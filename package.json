{
  "name": "react-calculator",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "big.js": "^5.2.2",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "build": "yarn sass:build && react-scripts build",
    "eject": "react-scripts eject",
    "sass:watch": "node-sass -w src/styles/scss -o src/styles/css",
    "sass:build": "node-sass src/styles/scss -o src/styles/css --source-map true",
    "start": "yarn run sass:build && react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "node-sass": "^4.12.0"
  }
}
